@import "../_variables.scss";




.icon {
  display: inline-block;
  font-weight: normal;
  color: $color-icon-medium;
  position: relative;
  overflow: hidden;
  width: 32px;
  height: 32px;
  text-indent: -9999px;
}

.icon:before,
.icon:after {
  font-size: 32px;
  speak: none;
  font-family: $font-icons;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  text-indent: 0px;
  display: block;
  position: absolute;
  word-spacing: 0;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon.actionable {
  color: $color-actionable;
  cursor: pointer;

}
.icon.actionable:hover:before {
  color: $color-actionable-hover;
}
.icon.actionable:active:before {
  color: $color-actionable-active;
}

.icon.alignMiddle {
  margin-top: -3px;
}

.icon.destructive,
.icon.actionable.destructive {
  color: $color-actionable-destructive;
}
.icon.actionable.destructive:hover:before {
  color: $color-actionable-destructive-hover;
}
.icon.actionable.destructive:active:before {
  color: $color-actionable-destructive-active;
}

.icon.disabled,
.icon.actionable.disabled {
  pointer-events: none;
  opacity: 0.3;
  color: $color-icon-medium;
}

.icon.pageHeader,
.icon.pageHeader:after,
.icon.white,
.icon.white:after {
  color: #FFFFFF;
}

.icon.light:before {
  color: $color-icon-light;
}

/*
  Necessary to override the legacy .dark style for highslide stuff
*/
.icon.dark,
.icon.dark:hover {
  background: none;
}

.icon.dark:before,
.icon.dark:hover:before {
  color: $color-icon-dark;
}


.icon.green:before,
.icon.green:hover:before {
  color: #6FAE3C;
}

.icon.red:before,
.icon.red:hover:before {
  color: $color-error-text;
}



